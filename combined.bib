@article{Lintott13AJ,
  author =       {Chris J. Lintott and Megan E. Schwamb and Thomas
                  Barclay and Charlie Sharzer and Debra A. Fischer and
                  John Brewer and Matthew Giguere and Stuart Lynn and
                  Michael Parrish and Natalie Batalha and Steve Bryson
                  and Jon Jenkins and Darin Ragozzine and Jason
                  F. Rowe and Kevin Schwainski and Robert Gagliano and
                  Joe Gilardi and Kian J. Jek and Jari-Pekka Pääkkönen
                  and Tjapko Smits},
  title =        {{Planet Hunters}: New {K}epler Planet Candidates
                  from Analysis of Quarter 2},
  journal =      {The Astronomical Journal},
  volume =       145,
  number =       6,
  pages =        151,
  url =          {http://stacks.iop.org/1538-3881/145/i=6/a=151},
  year =         2013,
  abstract =     {We present new planet candidates identified in NASA
                  Kepler Quarter 2 public release data by volunteers
                  engaged in the Planet Hunters citizen science
                  project. The two candidates presented here survive
                  checks for false positives, including examination of
                  the pixel offset to constrain the possibility of a
                  background eclipsing binary. The orbital periods of
                  the planet candidates are 97.46 days (KIC 4552729)
                  and 284.03 (KIC 10005758) days and the modeled
                  planet radii are 5.3 and 3.8 R ⊕ . The latter star
                  has an additional known planet candidate with a
                  radius of 5.05 R ⊕ and a period of 134.49 days,
                  which was detected by the Kepler pipeline. The
                  discovery of these candidates illustrates the value
                  of massively distributed volunteer review of the
                  Kepler database to recover candidates which were
                  otherwise uncataloged.}
}

