@Article{Camerer99,
  author =       {Camerer, Colin F and Hogarth, Robin M},
  title =        {The Effects of Financial Incentives in Experiments:
                  A Review and Capital-Labor-Production Framework},
  journal =      {Journal of Risk and Uncertainty},
  year =         1999,
  volume =       19,
  number =       {1-3},
  pages =        {7-42},
  month =        {December},
  abstract =     { We review 74 experiments with no, low, or high
                  performance-based financial incentives. The modal
                  result is no effect on mean performance (though
                  variance is usually reduced by higher
                  payment). Higher incentive does improve performance
                  often, typically judgment tasks that are responsive
                  so better effort. Incentives also reduce
                  \&quot;presentation\&quot; effects (e.g., generosity
                  and risk-seeking). Incentive effects are comparable
                  to effects of other variables, particularly
                  \&quot;cognitive capital\&quot; and task
                  \&quot;production\&quot; demands, and interact with
                  those variables, so a narrow-minded focus on
                  incentives alone is misguided. We also note that no
                  replicated study has made rationality violations
                  disappear purely by raising incentives. Copyright
                  1999 by Kluwer Academic Publishers},
  url =
                  {http://ideas.repec.org/a/kap/jrisku/v19y1999i1-3p7-42.html}
}

